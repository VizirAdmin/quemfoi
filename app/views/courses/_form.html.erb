<% form_for(@course) do |f| %>
  <%= render "shared/error_messages", :target => @course %>
  <p>
    <%= f.label :identifier %>
    <%= f.text_field :identifier %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :reference_code %>
    <%= f.text_field :reference_code %>
  </p>
  
  <p>
    <%= f.submit 'Salvar' %>
  </p>
<% end %>

